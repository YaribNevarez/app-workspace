#include "perftool.hpp"


PerformanceTool::PerformanceTool():
ControlApp()
{
	serverProcess = new ProcessTrigger(SERVER, this);
}

int PerformanceTool::server(void)
{
	for (;;)
	{}

	return 0;
}

int PerformanceTool::run(ProcessID ID)
{
	int rc = -1;
	switch(ID)
	{
	case SERVER:
		rc = server();
		break;

	default:;
	}
	return rc;
}

int PerformanceTool::run(void)
{
	serverProcess->run();

	return 0;
}

PerformanceTool::~PerformanceTool()
{}
